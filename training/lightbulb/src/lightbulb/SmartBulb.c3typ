extendable entity type SmartBulb extends LightBulb type key "SMRT_BLB" {
latitude: double
longitude:double
lumensUOM: Unit
powerUOM: Unit
temperatureUOM: Unit
voltageUOM: Unit

@db(timedValueHistoryField = "bulbStatusHistory", order="descending(to_UTC(timestamp))")
bulbStatus: SmartBulbStatus

bulbMeasurements: [SmartBulbMeasurementSeries](smartBulb, id)
bulbStatusHistory: [SmartBulbStatusSet] (smartBulb, id)
bulbEvents: [SmartBulbToEventRelation](smartBulb)


}