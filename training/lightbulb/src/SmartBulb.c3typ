entity type SmartBulb extends LightBulb mixes MetricEvaluatable type key "SMRT_BLB" {

  /***
  The latitude of this bulb
  */

  latitude: double

  longitude: double

  /***
  The unit of measurement used for this bulb's light output measurements.
  */
  lumensUOM: Unit
  powerUOM: Unit
  temperatureUOM: Unit
  voltageUOM: Unit

}
