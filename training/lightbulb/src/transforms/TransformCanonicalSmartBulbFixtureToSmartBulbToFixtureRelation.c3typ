type TransformCanonicalSmartBulbFixtureToSmartBulbToFixtureRelation mixes SmartBulbToFixtureRelation transforms CanonicalSmartBulbFixture {
    id:     ~ expression "concat(SN,fixture)"
    from:   ~ expression {id = "SN"}
    to:     ~ expression {id = "fixture"}
    start:  ~ expression "start"
    end:    ~ expression "end"
}